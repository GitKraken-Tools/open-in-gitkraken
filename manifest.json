{
  "name": "Open In GitKraken",
  "description": "Open public GitHub repositories in GitKraken",
  "manifest_version": 3,
  "version": "1.0.0",
  "web_accessible_resources": [
    {
      "resources": [ "assets/keif-dark.png" ],
      "matches": [ "https://github.com/*" ]
    }
  ],
  "icons": {
    "16": "assets/keif.png",
    "48": "assets/keif.png",
    "128": "assets/keif.png"
  },
  "content_scripts": [
    {
      "matches":["https://github.com/*/*"],
      "js":[
        "src/index.js"
      ],
      "css": [],
      "run_at": "document_end"
    }
  ]
}
